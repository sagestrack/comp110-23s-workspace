"""Example Functions"""

def my_max(number1: int, number2: int) -> int:
    """Returns maximum value out of two numbers"""
    if number1 >= number2:
        return number1
    else: #number1 < number2
        return number2

max_number: int = my_max(1,10)
other_max_number: int = my_max(10,3)
print(other_max_number)

"""In Class Practice"""

def mimic(my_words: str) -> str:
    """Given the string my_words, outputs the same string"""
    return my_words

mimic("Hello")
print(mimic("Hello"))
my_words: str = "Hello!"
response: str = mimic
print(response)

def mimic_letter(my_words2: str, letter_idx: int) -> str:
    """Outputs the character of my_words2 at index letter_idx"""
    if letter_idx >= len(my_words2):
        return("Too high of an index.")
    return my_words2[letter_idx]
